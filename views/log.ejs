<%
    if(results[0]!=undefined || results[0]!=null){
%>
    <table class="ui striped table">
        <thead>
            
            <tr>
                <!--키에 따른 열 추가 -->
                <th>timestamp</th>
<%
                var key = Object.keys(results[0]);
                for(let m in key){
                    
                    if(key[m] != null && key[m] !="timestamp"){
%>
                        <th><%= key[m]%></th>
<%
                    }
                }
%>
            </tr>

<!--로그에 따른 행 추가(value)-->
<%
            for(let i in results){
                var key = Object.keys(results[i])
                var value = Object.values(results[i])
%>
            
                <tr>
<%
                    for(var j=0; j<key.length; j++){
                        if(key.indexOf() != null){
%>
                            <td><%=value[j]%></td>
<%
                        }
                    }   
%>
                </tr>
<%
            }
%>
        </thead>
        <tbody id="my-tbody"></tbody>
    </table>
    
    
